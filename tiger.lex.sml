| 43 => (Tokens.RBRACE(yypos,yypos+1))
| 45 => (Tokens.LPAREN(yypos,yypos+1))
| 47 => (Tokens.RPAREN(yypos,yypos+1))
| 49 => (Tokens.LBRACK(yypos,yypos+1))
| 5 => (Tokens.MINUS(yypos,yypos+1))
| 51 => (Tokens.RBRACK(yypos,yypos+1))
